  "dialogue": [
    {
      "speaker": "natsukileft",
      "speakerAnimation": "neutral",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"...okay, you’ve convinced me.\""]
    },
    {
      "speaker": "yuriright",
      "speakerAnimation": "neutral",
      "box": "doki",
      "boxAnimation": "yuri",
      "text": ["\"Oh?\""]
    },
    {
      "speaker": "natsukileft",
      "speakerAnimation": "neutral",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"You better not tell this to ANYONE, but I sometimes sing in front of the mirror into my hairbrush.\""]
    },
    {
      "speaker": "natsukileft",
      "speakerAnimation": "hmmph",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"Only when my dad isn’t home though.\""]
    },
    {
      "speaker": "yuriright",
      "speakerAnimation": "smile",
      "box": "doki",
      "boxAnimation": "yuri",
      "text": ["\"...uhuhu.\""]
    },
    {
      "speaker": "natsukileft",
      "speakerAnimation": "angy",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"Hey! I heard that, you know!\""]
    },
    {
      "speaker": "yuriright",
      "speakerAnimation": "smile",
      "box": "doki",
      "boxAnimation": "yuri",
      "text": ["\"Its just... picturing you doing that, its quite an adorable mental image.\""]
    },
    {
      "speaker": "natsukileft",
      "speakerAnimation": "angy",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"S-shut it! And stop calling me cute! I-I’m not cute.\""]
    },
    {
      "speaker": "natsukileft",
      "speakerAnimation": "hmmph",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"Ugh, I shouldn’t have said anything.\""]
    },
    {
      "speaker": "natsukileft",
      "speakerAnimation": "neutral",
      "box": "doki",
      "boxAnimation": "natsu",
      "text": ["\"Let’s just get this started.\""]
    }
  ]
