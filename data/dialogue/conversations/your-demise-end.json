    {
      "speaker": "monika",
      "speakerAnimation": "monika_sad",
      "box": "ddlcpixel",
      "boxAnimation": "speaking",

      "text": ["I don't know where he got it."]
    },
    {
      "speaker": "monika",
      "speakerAnimation": "monika_sad",
      "box": "ddlcpixel",
      "boxAnimation": "speaking",

      "text": ["I don't even know how I got here, for that matter."]
