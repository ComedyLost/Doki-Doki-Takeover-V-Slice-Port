    {
      "speaker": "senpaiddto",
      "speakerAnimation": "hmm",
      "box": "rosesClassic",
      "boxAnimation": "enter",

      "text": ["So, you weren't the one who stole my mic?"]
    },
    {
      "speaker": "monika",
      "speakerAnimation": "monika_sad",
      "box": "ddlcpixel",
      "boxAnimation": "enter",

